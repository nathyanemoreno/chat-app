{
  "name": "with-docker",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "dotenv -- turbo run build",
    "clean": "turbo run clean",
    "dev": "dotenv -e .env.development turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "compose:up": "dotenv -e .env.development docker compose --profiles backend up mongo1 -d",
    "compose:down": "dotenv -e .env.development docker compose down mongo1",
    "compose:build": "dotenv -e .env.development docker compose --build --force-recreate --profiles backend up mongo1 -d -V",
    "compose:prune": "docker compose down -v --rmi all mongo1"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "dotenv-cli": "^7.4.1",
    "prettier": "^3.2.5",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "packageManager": "pnpm@8.15.5"
}
